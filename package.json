    "node-sass": "^4.12.0",
    "parallelshell": "^3.0.2",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-redux": "^7.1.1",
    "react-scripts": "3.1.1",
    "redux": "^4.0.4",
    "sass": "^1.23.0-module.beta.1",
